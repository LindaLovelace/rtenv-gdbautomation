source qemudbg_init.in
set logging on
set strcpy(cmd[0], "echo -n \"A B\"")
set strcpy(cmd[1], "echo")
set strcpy(cmd[2], "-n")
set strcpy(cmd[3], "A B")
echo Processed command:\n
p cmd[0]
echo Tokens:\n
set $r0=1
while $r0<=3
      p cmd[$r0++]
end
set $r0=cmdtok(cmd[0])
set $r1=1
while $r0
   echo Testing token:\n
   p cmd[$r1]
   if !strcmp($r0, cmd[$r1++])
      echo Pass: this token is correct\n
   else
      echo Fail: this token is wrong\n
      p $r0
   end
   set $r0=cmdtok(NULL)
end

