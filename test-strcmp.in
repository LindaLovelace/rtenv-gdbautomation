source qemudbg_init.in
set logging on
set strcpy(cmd[0], "def")
set strcpy(cmd[1], "def")
set $r0=strcmp(cmd[0], cmd[1])
if !$r0
   echo Pass: strcmp("def", "def") is 0\n
else
   echo Fail: strcmp("def", "def") shall be 0\n
   p $r0
end
set cmd[0][0]='c'
set $r0=strcmp(cmd[0], cmd[1])
if $r0<0
   echo Pass: strcmp("cef", "def") returns negative value\n
else
   echo Fail: strcmp("cef", "def") is non-negative\n
   p $r0
end
set cmd[0][0]='g'
set $r0=strcmp(cmd[0], cmd[1])
if $r0>0
   echo Pass: strcmp("gef", "def") returns positive value\n
else
   echo Fail: strcmp("gef", "def") is non-positive\n
   p $r0
end

